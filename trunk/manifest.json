{
  "version": "1.0",
  "name": "media",
  "description": "media",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*","ftp://*/*", "file:///*"],
    "js": ["floatingBar.js"],
    "css": ["style.css"],
    "run_at":"document_start"
  }],

